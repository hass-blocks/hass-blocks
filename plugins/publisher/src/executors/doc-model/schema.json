{
  "$schema": "https://json-schema.org/schema",
  "version": 2,
  "title": "ApiExtractor executor",
  "description": "",
  "type": "object",
  "properties": {
    "mainEntrypointFile": {
      "type": "string",
      "description": "The main entrypoint for your package API"
    },
    "projectFolder": {
      "type": "string",
      "description": "Folder containing the package"
    },
    "docModelFolder": {
      "type": "string",
      "description": "Folder doc model json file is generated in. Will be created if it doesn't exist"
    },
    "exports": {
      "type": "array",
      "items": {
        "type": "object",
        "output": {
          "type": "string"
        },
        "entry": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      }
    }
  },
  "required": []
}
